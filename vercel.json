{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": "./prisma/prisma/dev.sqlite"
        },
        "hooks": {
            "build": "npx prisma generate"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  