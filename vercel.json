{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": "./prisma/prisma/dev.sqlite"
        }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/docs"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
}
  